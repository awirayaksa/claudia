{
  "name": "claudia",
  "version": "0.1.8",
  "description": "Claude Desktop clone that connects to Open WebUI/Open Router",
  "main": "dist-electron/main.js",
  "author": "Adri Wirayaksa",
  "license": "MIT",
  "homepage": "https://github.com/awirayaksa/claudia#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/awirayaksa/claudia.git"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:electron": "npm run build && electron-builder",
    "build:win": "npm run build && electron-builder --win",
    "preview": "vite preview",
    "lint": "eslint src electron --ext ts,tsx",
    "format": "prettier --write \"src/**/*.{ts,tsx,css}\" \"electron/**/*.ts\"",
    "type-check": "tsc --noEmit",
    "release": "powershell -ExecutionPolicy Bypass -File ./scripts/release.ps1",
    "release:bat": "scripts\\release.bat"
  },
  "dependencies": {
    "@mcp-ui/client": "^5.17.1",
    "@modelcontextprotocol/sdk": "^1.24.3",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-tooltip": "^1.0.7",
    "@reduxjs/toolkit": "^2.0.0",
    "@types/react-syntax-highlighter": "^15.5.13",
    "axios": "^1.6.0",
    "clsx": "^2.0.0",
    "date-fns": "^3.0.0",
    "electron-store": "^8.1.0",
    "eventsource-parser": "^1.1.0",
    "file-type": "^18.5.0",
    "framer-motion": "^10.16.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.0",
    "react-markdown": "^10.1.0",
    "react-redux": "^9.0.0",
    "react-router-dom": "^6.20.0",
    "react-syntax-highlighter": "^16.1.0",
    "rehype-raw": "^7.0.0",
    "remark-gfm": "^4.0.1",
    "tailwind-merge": "^2.1.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.0",
    "autoprefixer": "^10.4.16",
    "electron": "^28.0.0",
    "electron-builder": "^24.9.0",
    "eslint": "^8.55.0",
    "png-to-ico": "^3.0.1",
    "postcss": "^8.4.32",
    "prettier": "^3.1.0",
    "sharp": "^0.34.5",
    "tailwindcss": "^3.4.0",
    "to-ico": "^1.1.5",
    "typescript": "^5.3.0",
    "vite": "^5.0.0",
    "vite-plugin-electron": "^0.28.0",
    "vite-plugin-electron-renderer": "^0.14.0"
  }
}
